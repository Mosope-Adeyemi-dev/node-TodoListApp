<%- include('header') -%>

<body>
    <div class="box" id="heading">
        <h1> <%= listType %>  </h1>
    </div>

    <%# comment in ejs %>
    <div class="box">
        <% for(let i = 0;i<items.length;i++){ %>
        <div class="item">
            <input type="checkbox">
            <p> <%= items[i] %> </p>
        </div>
        <% } %>

        <form action="/" class="item" method="post">
            <input type="text" name="inpt" placeholder="New Item">
            <button type="submit" name="submit" value= <%= listType %>>+</button>
        </form>

    </div>

</body>
 <%- include('footer') -%>